@using br.com.arcnet.spedstockweb.Models.Shared
@model br.com.arcnet.spedstockweb.Models.Shared.MessageModel
@{
    Layout = null;
}

<div id="modal_message" class="modal hide alert-success fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-message-type="@Model.MessageType">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel">@Model.MessageTitle</h3>
    </div>
    <div class="modal-body">
        <p>@Model.Message</p>
    </div>
    <div class="modal-footer"> 
        <button id="confirma" class="btn" data-dismiss="modal" aria-hidden="true">OK</button> 
   
    </div>
</div>

